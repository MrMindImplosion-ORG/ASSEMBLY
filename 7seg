; Set numbers

CALL SETUP
LOOPY:
  CALL COUNTER
JMP LOOPY

SETUP:
  MOVW 0X00
  MOVWR TRISC
  MOVW 0XFF
  MOVWR PORTC
  
  MOVW 0x7D
  MOVWR B0
  
  MOVW 0X05
  MOVWR B1
  
  MOVW 0X5B
  MOVWR B2
  
  MOVW 0X4F
  MOVWR B3
  
  MOVW 0X27
  MOVWR B4
  
  MOVW 0X6E
  MOVWR B5
  
  MOVW 0X7E
  MOVWR B6
  
  MOVW 0X45
  MOVWR B7
  
  MOVW 0X7F
  MOVWR B8
  
  MOVW 0X6F
  MOVWR B9
RET

SETR0:
  MOVRW B0
  MOVWR B26
RET

SETR1:
  MOVRW B1
  MOVWR B26
RET

SETR2:
  MOVRW B2
  MOVWR B26
RET

SETR3:
  MOVRW B3
  MOVWR B26
RET

SETR4:
  MOVRW B4
  MOVWR B26
RET

SETR5:
  MOVRW B5
  MOVWR B26
RET

SETR6:
  MOVRW B6
  MOVWR B26
RET

SETR7:
  MOVRW B7
  MOVWR B26
RET

SETR8:
  MOVRW B8
  MOVWR B26
RET

SETR9:
  MOVRW B9
  MOVWR B26
RET

SETL0:
  MOVRW B0
  MOVWR B27
RET

SETL1:
  MOVRW B1
  MOVWR B27
RET

SETL2:
  MOVRW B2
  MOVWR B27
RET

SETL3:
  MOVRW B3
  MOVWR B27
RET

SETL4:
  MOVRW B4
  MOVWR B27
RET

SETL5:
  MOVRW B5
  MOVWR B27
RET

SETL6:
  MOVRW B6
  MOVWR B27
RET

SETL7:
  MOVRW B7
  MOVWR B27
RET

SETL8:
  MOVRW B8
  MOVWR B27
RET

SETL9:
  MOVRW B9
  MOVWR B27
RET

DISPLAY:
  MOVW 0X0F
  MOVWR B25

  DISPLAYSUBA:
    MOVW 0X00
    MOVWR PORTC
    MOVW 0X00
    MOVWR PORTB

    MOVW 0X40
    MOVWR PORTC
    MOVRW B27
    MOVWR PORTB
    
    DEC B25
    JPZ DISPLAYEND
    JMP DISPLAYSUBB

  DISPLAYSUBB:
    MOVW 0X00
    MOVWR PORTC
    MOVW 0X00
    MOVWR PORTB


    MOVW 0X80
    MOVWR PORTC
    MOVRW B26
    MOVWR PORTB 
    
    DEC B25
    JPZ DISPLAYEND
    JMP DISPLAYSUBA
    
  DISPLAYEND:
    MOVW 0X00
    MOVWR PORTC
    RET

DELAY:
  MOVW 0XFF
  DELAYSUB:
    SUBW 0X01
    JPZ DELAYEND
    JMP DELAYSUB
  DELAYEND:
    RET

COUNTER:
  CALL SETL0
  CALL COUNTR
  CALL SETL1
  CALL COUNTR
  CALL SETL2
  CALL COUNTR
  CALL SETL3
  CALL COUNTR
  CALL SETL4
  CALL COUNTR
  CALL SETL5
  CALL COUNTR
  CALL SETL6
  CALL COUNTR
  CALL SETL7
  CALL COUNTR
  CALL SETL8
  CALL COUNTR
  CALL SETL9
  CALL COUNTR
JMP COUNTER
  
COUNTR:
  CALL SETR0
  CALL DISPLAY
  CALL SETR1
  CALL DISPLAY
  CALL SETR2
  CALL DISPLAY
  CALL SETR3
  CALL DISPLAY
  CALL SETR4
  CALL DISPLAY
  CALL SETR5
  CALL DISPLAY
  CALL SETR6
  CALL DISPLAY
  CALL SETR7
  CALL DISPLAY
  CALL SETR8
  CALL DISPLAY
  CALL SETR9
  CALL DISPLAY
RET
  
